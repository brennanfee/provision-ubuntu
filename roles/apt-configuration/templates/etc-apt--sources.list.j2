# APT Sources
# {{ ansible_managed }}

# See http://help.ubuntu.com/community/UpgradeNotes for how to upgrade to
# newer versions of the distribution.
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} main restricted
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} main restricted
{% endif %}

## Major bug fix updates produced after the final release of the
## distribution.
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates main restricted
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates main restricted
{% endif %}

## N.B. software from this repository is ENTIRELY UNSUPPORTED by the Ubuntu
## team, and may not be under a free licence. Please satisfy yourself as to
## your rights to use the software. Also, please note that software in
## universe WILL NOT receive any review or updates from the Ubuntu security
## team.
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} universe
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} universe
{% endif %}
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates universe
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates universe
{% endif %}

## N.B. software from this repository is ENTIRELY UNSUPPORTED by the Ubuntu
## team, and may not be under a free licence. Please satisfy yourself as to
## your rights to use the software. Also, please note that software in
## multiverse WILL NOT receive any review or updates from the Ubuntu
## security team.
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} multiverse
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }} multiverse
{% endif %}
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates multiverse
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-updates multiverse
{% endif %}

{% if apt_include_backports %}
## N.B. software from this repository may not have been tested as
## extensively as that contained in the main release, although it includes
## newer versions of some applications which may provide useful features.
## Also, please note that software in backports WILL NOT receive any review
## or updates from the Ubuntu security team.
deb http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-backports main restricted universe multiverse
{% if apt_include_source %}
deb-src http://archive.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-backports main restricted universe multiverse
{% endif %}
{% endif %}

{% if apt_include_partners %}
## Uncomment the following two lines to add software from Canonical's
## 'partner' repository.
## This software is not part of Ubuntu, but is offered by Canonical and the
## respective vendors as a service to Ubuntu users.
deb http://archive.canonical.com/ubuntu {{ ansible_lsb.codename }} partner
{% if apt_include_source %}
deb-src http://archive.canonical.com/ubuntu {{ ansible_lsb.codename }} partner
{% endif %}
{% endif %}

deb http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security main restricted
{% if apt_include_source %}
deb-src http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security main restricted
{% endif %}
deb http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security universe
{% if apt_include_source %}
deb-src http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security universe
{% endif %}
deb http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security multiverse
{% if apt_include_source %}
deb-src http://security.ubuntu.com/ubuntu {{ ansible_lsb.codename }}-security multiverse
{% endif %}
